{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash": false,
    "rewrites": [
      {
        "source": "/resume",
        "destination": "/media/resume.pdf"
      },
      {
        "source": "/pcr",
        "destination": "/media/PCR.pdf"
      }
    ]
  }
}
